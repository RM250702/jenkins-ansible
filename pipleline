pipeline{
    agent any
    stages{
        stage("SCM Checkout"){
            steps{
                 git 'https://github.com/RM250702/ansibleplybook'
                 
            }
        }
        stage('Execute ansible playbook'){
            steps{
                ansiblePlaybook credentialsId: '78d491e8-b3c6-4fcf-8f6e-ec456fcd0579', disableHostKeyChecking: true, installation: 'ansible', playbook: 'test-playbook.yaml'
            }
            
        }
    }
}
