pipeline{
    agent any
    stages{
        stage("SCM Checkout){
            steps{
                 git 'https://github.com/RM250702/ansibleplybook'
                 
            }
        }
        stage('Execute ansible playbook'){
            steps{
                ansiblePlaybook credentialsID: 'private-key', diableHostKeyChecking: true, installation: 'ansible', inventory: 'variable', playbook: test-playbook.yaml
            }
            
        }
    }
}
